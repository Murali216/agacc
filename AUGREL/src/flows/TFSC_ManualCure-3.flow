    <description>This flow is created for manual cure request process.</description>
    <formulas>
        <name>CharacterCount</name>
        <dataType>Number</dataType>
        <expression>LEN({!NoteGeneration})+5</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>DefaultForm</name>
        <dataType>String</dataType>
        <expression>IF(AND({!BorrowerGarageState}=&apos;WI&apos;,{!ContractStateDateComparison} &lt;= {!ContractDate}),&apos;4137WI&apos;,
IF(AND(NOT(ISBLANK({!ExpiryDate})), {!BorrowerGarageState}=&apos;TX&apos;, ({!$Flow.CurrentDate} - {!ExpiryDate})&gt;0, ({!$Flow.CurrentDate} - {!ExpiryDate})&lt;=15),
	&apos;4040TX&apos;,
	{!CustomSettingDefaultForm}))</expression>
    </formulas>
    <formulas>
        <name>FinalRemainingBalance</name>
        <dataType>Number</dataType>
        <expression>IF(
 {!RecordTypeNameFA} = {!Retail},
 {!RemainingBalanceValue},
 {!GrossPayoffAmtValue}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>GrossPayoffAmtValue</name>
        <dataType>Number</dataType>
        <expression>IF(
 ISBLANK({!GrossPayOffAmt}),
 0.00,
 VALUE({!GrossPayOffAmt})
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>RemainingBalanceValue</name>
        <dataType>Number</dataType>
        <expression>IF(
 ISBLANK({!RemainingBalance}),
 0.00,
 VALUE({!RemainingBalance})
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>StateThresholdsCS</name>
        <dataType>String</dataType>
        <expression>IF(OR({!BorrowerGarageState} = &apos;CA&apos;, {!BorrowerGarageState} = &apos;MI&apos;),
	IF({!CoBorrowerExist},{!BorrowerGarageState}&amp;&apos; - CoBorr&apos;,{!BorrowerGarageState}&amp;&apos; - Borr&apos;),
	IF(AND({!BorrowerGarageState} = &apos;TX&apos;,{!ContractStateValid} ),{!BorrowerGarageState}&amp;&apos; - CS&apos;,{!BorrowerGarageState})
)</expression>
    </formulas>
    <formulas>
        <name>testboolean</name>
        <dataType>Boolean</dataType>
        <expression>AND(NOT(ISBLANK({!ExpiryDate})), {!BorrowerGarageState}=&apos;TX&apos;, ({!$Flow.CurrentDate} - {!ExpiryDate})&gt;0, ({!$Flow.CurrentDate} - {!ExpiryDate})&lt;=15)</expression>
    </formulas>
    <formulas>
        <description>Formula field to append first name and last name of running user.</description>
        <name>UserName</name>
        <dataType>String</dataType>
        <expression>{!UserFirstName}&amp;&apos; &apos;&amp;{!UserLastName}</expression>
    </formulas>
