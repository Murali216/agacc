    <decisions>
        <name>CollateralFound</name>
        <label>CollateralFound</label>
        <locationX>322</locationX>
        <locationY>482</locationY>
        <defaultConnector>
            <targetReference>CreateNewTitle2</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>CollateralTrue</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FA</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Collateral</targetReference>
            </connector>
            <label>CollateralTrue</label>
        </rules>
    </decisions>
    <decisions>
        <name>NoTitleorFA</name>
        <label>NoTitleorFA</label>
        <locationX>76</locationX>
        <locationY>37</locationY>
        <defaultConnector>
            <targetReference>LookUpStateToState</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>NoTitleorFAProvided</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>orgFinancialAccount</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>OriginalTitle</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_Error</targetReference>
            </connector>
            <label>NoTitleorFAProvided</label>
        </rules>
    </decisions>
    <decisions>
        <name>STSCaseFound</name>
        <label>STSCaseFound</label>
        <locationX>338</locationX>
        <locationY>148</locationY>
        <defaultConnector>
            <targetReference>No_State_to_State_Case_Found</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>FoundSTS</name>
            <conditionLogic>1 AND (2 OR 3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>STSCase</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>STSCase.Service_Request_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>State Transfer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>STSCase.Service_Request_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>SOC</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>STSCase.Service_Request_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Duplicate Request</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_New_Title</targetReference>
            </connector>
            <label>FoundSTS</label>
        </rules>
    </decisions>
